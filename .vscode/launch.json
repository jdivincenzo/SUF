    "version": "0.2.0",
    "configurations": [

        {
            "type": "pwa-chrome",
            "request": "launch",
            "name": "Launch Chrome against localhost",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Launch Firefox against localhost",
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "url": "http://localhost:5000/api/post/GetNearbyPosts?lat=-34.629835&lon=-58.694141&dist=10",
            "webRoot": "${workspaceFolder}"
        }
    ]
}